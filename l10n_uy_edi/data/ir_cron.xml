<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <record id="ir_cron_get_ucfe_notif" model="ir.cron">
            <field name="name">Consult DGI State of Waiting CFE</field>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_account_move"/>
            <field name="state">code</field>
            <field name="code">model.search([('l10n_uy_cfe_state', 'in', ['received'])]).action_l10n_uy_get_dgi_state()</field>
            <field name="active" eval="True"/>
        </record>

        <record id="ir_cron_get_vendor_bills_received" model="ir.cron">
            <field name="name">UY: Create vendor bills (sync from Uruware)</field>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_account_move"/>
            <field name="state">code</field>
            <field name="code">model.cron_l10n_uy_get_vendor_bills()</field>
            <field name="active" eval="True"/>
        </record>

    </data>
</odoo>
