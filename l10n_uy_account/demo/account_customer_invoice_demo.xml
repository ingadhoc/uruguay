        <value model="account.tax" eval="{'tax_ids': [(4, obj().search([('company_id', '=', ref('company_uy')), ('type_tax_use', '=', 'purchase'), ('description', '=', 'VAT 10%')], limit=1).id)]}"/>
    </function>


